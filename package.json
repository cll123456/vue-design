{
  "name": "vue-design",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "eslint": "eslint --ext .js,ts,.vue --fix src",
    "prettier": "prettier . --write",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --coverage"
  },
  "dependencies": {
    "element-plus": "^2.1.11",
    "pinia": "^2.0.14",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.11",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@unocss/preset-attributify": "^0.33.1",
    "@unocss/preset-icons": "^0.33.1",
    "@unocss/preset-typography": "^0.33.1",
    "@unocss/preset-web-fonts": "^0.33.1",
    "@unocss/reset": "^0.33.1",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/test-utils": "^2.0.0-rc.21",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.1",
    "picocolors": "^1.0.0",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "simple-git-hooks": "^2.7.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "unocss": "^0.33.1",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.8",
    "vitest": "^0.12.0",
    "vue-tsc": "^0.34.11"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "pnpm exec ts-node scripts/verifyCommit.ts $1"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "pnpm run eslint",
      "pnpm run prettier"
    ]
  }
}
